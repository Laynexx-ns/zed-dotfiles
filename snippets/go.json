{
  "create func 0return": {
    "prefix": "f0",
    "body": ["func $1(){", "", "}"],
    "description": "uwu"
  },
  "create func 1return": {
    "prefix": "f1",
    "body": ["func $1() $2 {", "", "}"],
    "description": "uwu"
  },
  "create func 2return": {
    "prefix": "f2",
    "body": ["func $1() ($2, $3) {", "", "}"],
    "description": "uwu"
  },
  "create func for struct": {
    "prefix": "fs",
    "body": ["func ($1 *$2) $3($4) $5 {", "", "}"],
    "description": "creates function for structure"
  },
  "create func with two return vars for struct": {
    "prefix": "fs2",
    "body": ["func ($1 *$2) $3($4) ($5 $6) {", "", "}"],
    "description": "creates function for structure"
  },
  "interface": {
    "prefix": ["interface", "face"],
    "body": ["type $1 interface {", "", "}"],
    "description": "just creates interface"
  },
  "create struct": {
    "prefix": "struct",
    "body": ["type $1 struct  {", "  $2", "}"],
    "description": "meow"
  },
  "err nil check": {
    "prefix": ["iferr", "ie", "ife"],
    "body": ["if err != nil {", "  $1", "}"]
  },
  "err with 1 new nil check": {
    "prefix": ["iev", "ifev"],
    "body": ["if err := ${1:expression}($2); err != nil {$3}"]
  },
  "err with 2 new nil check": {
    "prefix": ["iferr2", "iev2", "ife2"],
    "body": ["if _, err := ${1:expression}($2); err != nil {$3}"]
  },
  "if err return": {
    "prefix": ["iferreturn", "ier", "iferet"],
    "body": ["if err != nil", "  return $3, err", "}"]
  },
  "if err2 return": {
    "prefix": ["iferreturn2", "ier2", "iferet2"],
    "body": [
      "if _, err := ${1:expression}($2); err != nil",
      "  return $3, err",
      "}"
    ]
  }
}
