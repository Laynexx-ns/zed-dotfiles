[
  {
    "label": "execute cpp-11",
    "command": "sh ~/.config/zed/scripts/cpp/cpp-11.sh",
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "show_command": true
 },
 {
   "label": "Build C++ program",
   "command": "g++",
   "args": ["-g", "main.cpp", "-o", "main"],
   "cwd": "$ZED_WORKTREE_ROOT"
 },
 {
   "label": "bun run dev",
   "command": "sh ~/.config/zed/scripts/ts/run-dev.sh",
   "use_new_terminal": true,
   "allow_concurrent_runs": false,
   "reveal": "always",
   "show_command": true
 }
]
